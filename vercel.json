{
  "functions": {
    "app/api/**/*": {
      "maxDuration": 60 
    }
  },
  "rewrites": [
     {
        source: "/api/auth/:path*",
        destination: "/api/auth/:path*",
      },
      {
        "source": "/api/:path*",
        "destination": "https://admin.ktravel.io.vn/api/:path*"
      }
  ]
}
